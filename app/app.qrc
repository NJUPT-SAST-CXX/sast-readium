<!DOCTYPE RCC>
<RCC version="1.0">
    <qresource prefix="/images">
        <file alias="icon">../assets/images/icon.ico</file>
        <file alias="logo">../assets/images/logo.svg</file>
        <file alias="logo-dark">../assets/images/logo-dark.svg</file>
    </qresource>

    <qresource prefix="/images/filetypes">
        <file alias="pdf.svg">../assets/images/filetypes/pdf.svg</file>
        <file alias="epub.svg">../assets/images/filetypes/epub.svg</file>
        <file alias="txt.svg">../assets/images/filetypes/txt.svg</file>
        <file alias="doc.svg">../assets/images/filetypes/doc.svg</file>
        <file alias="default.svg">../assets/images/filetypes/default.svg</file>
    </qresource>

    <qresource prefix="/images/dark">
        <file alias="theme">../assets/images/dark/theme.png</file>
    </qresource>

    <qresource prefix="/images/light">
        <file alias="theme">../assets/images/light/theme.png</file>
    </qresource>

    <qresource prefix="/styles">
        <file alias="dark.qss">../assets/styles/dark.qss</file>
        <file alias="light.qss">../assets/styles/light.qss</file>
        <file alias="thumbnails.qss">../assets/styles/thumbnails.qss</file>
        <file alias="common.qss">../assets/styles/common.qss</file>
    </qresource>
</RCC>
